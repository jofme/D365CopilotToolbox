			<Method>
				<Name>exist</Name>
				<Source><![CDATA[
    /// <summary>
    /// Returns true when a mapping exists for the specified application area.
    /// </summary>
    public static boolean exist(COTXCopilotAgentApplicationArea _applicationArea)
    {
        return _applicationArea && (select firstonly RecId from COTXCopilotAgentApplicationAreas
            where COTXCopilotAgentApplicationAreas.ApplicationArea == _applicationArea).RecId != 0;
    }

]]></Source>
