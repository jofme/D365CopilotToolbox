				<Source><![CDATA[
    /// <summary>
    /// Finds the application-area mapping for the specified application area.
    /// </summary>
    public static COTXCopilotAgentApplicationAreas find(COTXCopilotAgentApplicationArea _applicationArea, boolean _forUpdate = false)
    {
        COTXCopilotAgentApplicationAreas record;

        record.selectForUpdate(_forUpdate);

        select firstonly record
            where record.ApplicationArea == _applicationArea;

        return record;
    }

]]></Source>
